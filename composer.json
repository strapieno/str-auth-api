{
  "name": "strapieno/str-auth-api",
  "homepage": "https://github.com/strapieno/str-auth-api",
  "require": {
    "php": ">=5.5.0",
    "zfcampus/zf-mvc-auth": "~1.0",
    "matryoshka-model/matryoshka": "0.8.*@dev",
    "matryoshka-model/mongo-wrapper": "0.8.*@dev",
    "matryoshka-model/zf2-matryoshka-module": "0.8.*@dev",
    "matryoshka-model/zf-apigility-matryoshka": "dev-develop",
    "ripaclub/aclman": "dev-develop@dev",
    "strapieno/str-auth-model" : "dev-develop"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.7",
    "zendframework/zftool": "dev-master",
    "robertboloc/zf2-components-list-generator": "dev-master",
    "satooshi/php-coveralls": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Strapieno\\Auth\\Api\\": "src/",
      "Strapieno\\Auth\\ApiTest\\": "tests/"
    },
    "classmap": [
      "./Module.php"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:matryoshka-model/zf-apigility-matryoshka.git"
    }
  ],
  "authors": [
    {
      "name": "Antonio Visalli",
      "email": "antonino.visalli@gmail.com",
      "homepage": "http://github.com/visa4"
    }
  ],
  "minimum-stability": "dev"
}
